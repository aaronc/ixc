{
    "name": "IXC",
    "scopeName": "source.ixc",
    "patterns": [
      {
        "include": "#comments"
      },
      {
        "include": "#keywords"
      },
      {
        "include": "#identifiers"
      },
      {
        "include": "#strings"
      }
    ],
    "repository": {
      "comments": {
        "patterns": [
          {
            "match": "//.*$",
            "name": "comment.line.double-slash.ixc"
          }
        ]
      },
      "keywords": {
        "patterns": [
          {
            "match": "\\b(interface|struct|event|tx|query|object|impl|for|in|map|client|return|emits|key|transfer|ref|mut|account_scoped|var)\\b",
            "name": "keyword.control.ixc"
          }
        ]
      },
      "identifiers": {
        "patterns": [
          {
            "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b",
            "name": "variable.other.ixc"
          }
        ]
      },
      "strings": {
        "patterns": [
          {
            "match": "\"[^\"]*\"",
            "name": "string.quoted.double.ixc"
          }
        ]
      },
      "brackets": {
        "patterns": [
          {
            "match": "[\\[\\]\\{\\}\\(\\)]",
            "name": "punctuation.section.brackets.ixc"
          }
        ]
      }
    }
  }